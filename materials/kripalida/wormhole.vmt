"UnlitGeneric"
{
	"$basetexture" "kripalida/wormhole"
	"$envmap" "env_cubemap"
	"$envmaptint" "[.1 .1 .2]"
	"$envmapconstrast" "1"
	"$selfillum" "1"

	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 30.00
		}
	}
}